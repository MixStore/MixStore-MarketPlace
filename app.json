{
  "expo": {
    "name": "MixStore",
    "slug": "mixstore",
    "scheme": "mixstore",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "platforms": ["ios", "android", "web"],
    "web": {
      "favicon": "./assets/images/logo.png",
      "manifest": {
      "short_name": "MixStore",
      "name": "MixStore",
      "start_url": ".",
      "display": "standalone",
      "bundler": "metro",
      "orientation": "portrait",
      "background_color": "#ffffff",
      "theme_color": "#000000"
      }
    },

    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.jonathanrebello01.mixstore"
    },
    "plugins": [
      "expo-secure-store",
      "expo-router",
      [
        "expo-image-picker",
        {
          "photosPermission": "O app Precisa acessar Fotos em seu dispositivo"
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "6f215fda-ca97-4885-8acc-b5ec20441ca4"
      }
    },
    "owner": "jonathanrebello01"
  }
}
